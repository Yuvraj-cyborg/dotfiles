{
  "language_models": {
    "openai_compatible": {
      "opanai": {
        "api_url": "http://localhost:3000/v1",
        "available_models": [
          {
            "name": "gpt-occ",
            "display_name": null,
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": true,
              "prompt_cache_key": true
            }
          }
        ]
      },
      "gpt-120": {
        "api_url": "http://localhost:3000/v1/chat/completions",
        "available_models": [
          {
            "name": "gpt-120b-oss",
            "display_name": null,
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": true,
              "prompt_cache_key": true
            }
          }
        ]
      },
      "gpt-120b": {
        "api_url": "http://localhost:3000/v1",
        "available_models": [
          {
            "name": "gpt-120b",
            "display_name": null,
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": true,
              "prompt_cache_key": true
